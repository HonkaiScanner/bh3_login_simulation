        <meta-data
            android:name="DC_APPKEY"
            android:value="02522a2b2726fb0a03bb19f2d8d9524d" />
        <meta-data
            android:name="APP_KEY"
            android:value="n7a5gpvhzkny" />
        <meta-data
            android:name="APP_SECRET"
            android:value="ggm5oqotrjs8spkgwk5qjw5447bj7zl5" />
