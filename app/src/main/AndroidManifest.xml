        <meta-data
            android:name="is_offline_game"
            android:value="false" /> <!-- true:单机游戏 false:网游 -->
        <meta-data
            android:name="app_key"
            android:value="7NEgcusdF1ss0cK8CSwo8Ksk0" />
        <meta-data
            android:name="vivo_union_sdk"
            android:value="4.6.0.1" />

        <activity
